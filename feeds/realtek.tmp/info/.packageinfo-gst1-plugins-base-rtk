Config:
    menu "Select GStreamer base modules and libraries"
      depends on PACKAGE_gstreamer1-plugins-base-rtk


    config gst1-plugins-base-rtk-all
      bool "Include all GStreamer base plugins"
      select PACKAGE_gst1-plugins-base-rtk

    comment "Modules"

    config PACKAGE_gst1-mod-alsa-rtk
      prompt "GStreamer alsa module"

     config PACKAGE_gst1-mod-app-rtk
      prompt "GStreamer app module"

     config PACKAGE_gst1-mod-audioconvert-rtk
      prompt "GStreamer audioconvert module"

     config PACKAGE_gst1-mod-audiorate-rtk
      prompt "GStreamer audiorate module"

     config PACKAGE_gst1-mod-audioresample-rtk
      prompt "GStreamer audioresample module"

     config PACKAGE_gst1-mod-audiotestsrc-rtk
      prompt "GStreamer audiotestsrc module"

     config PACKAGE_gst1-mod-playback-rtk
      prompt "GStreamer playback module"

     config PACKAGE_gst1-mod-gio-rtk
      prompt "GStreamer gio module"

     config PACKAGE_gst1-mod-ogg-rtk
      prompt "GStreamer ogg module"

     config PACKAGE_gst1-mod-tcp-rtk
      prompt "GStreamer tcp module"

     config PACKAGE_gst1-mod-theora-rtk
      prompt "GStreamer theora module"

     config PACKAGE_gst1-mod-typefindfunctions-rtk
      prompt "GStreamer typefindfunctions module"

     config PACKAGE_gst1-mod-videoconvert-rtk
      prompt "GStreamer videoconvert module"

     config PACKAGE_gst1-mod-videorate-rtk
      prompt "GStreamer videorate module"

     config PACKAGE_gst1-mod-videoscale-rtk
      prompt "GStreamer videoscale module"

     config PACKAGE_gst1-mod-videotestsrc-rtk
      prompt "GStreamer videotestsrc module"

     config PACKAGE_gst1-mod-volume-rtk
      prompt "GStreamer volume module"

     config PACKAGE_gst1-mod-vorbis-rtk
      prompt "GStreamer vorbis module"

   

    comment "Libraries"

    config PACKAGE_libgst1allocators-rtk
      prompt "GStreamer allocators library"

     config PACKAGE_libgst1app-rtk
      prompt "GStreamer app library"

     config PACKAGE_libgst1audio-rtk
      prompt "GStreamer audio library"

     config PACKAGE_libgst1fft-rtk
      prompt "GStreamer fft library"

     config PACKAGE_libgst1pbutils-rtk
      prompt "GStreamer pbutils library"

     config PACKAGE_libgst1riff-rtk
      prompt "GStreamer riff library"

     config PACKAGE_libgst1rtp-rtk
      prompt "GStreamer rtp library"

     config PACKAGE_libgst1rtsp-rtk
      prompt "GStreamer rtsp library"

     config PACKAGE_libgst1sdp-rtk
      prompt "GStreamer sdp library"

     config PACKAGE_libgst1tag-rtk
      prompt "GStreamer tag library"

     config PACKAGE_libgst1video-rtk
      prompt "GStreamer video library"

   

  endmenu

